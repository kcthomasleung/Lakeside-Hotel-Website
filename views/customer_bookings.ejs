<%- include('head')  %>
<%- include('header') %>


<h2> Customer Room Bookings </h2>

<span class="results"> Number of Room Bookings: <%= count %> </span> 

<br /><br />

<table class="bookings-table">
    <thead>
        <tr>
            <th>Customer Number</th>
            <th>Customer Name</th>
            <th>Customer Email</th>
            <th>Customer Address</th>
            <th>Card Type</th>
            <th>Card Expiry</th>
            <th>Card Number</th>
            <th>Booking Reference</th>
            <th>Booking Cost</th>
            <th>Outstanding</th>
            <th>Notes</th>
            <th>Room Number</th>
            <th>Checkin Date</th>
            <th>Checkout Date</th>
        </tr>
    </thead>
    <tbody>   
        <% for (x in data) {  %>
        <% let u = data[x]; %>
        <tr>
            <td><%= u.c_no %></td>
            <td><%= u.c_name %></td>
            <td><%= u.c_email %></td>
            <td><%= u.c_address %></td>
            <td><%= u.c_cardtype %></td>
            <td><%= u.c_cardexp %></td>
            <td><%= u.c_cardno %></td>
            <td><%= u.b_ref %></td>
            <td><%= u.b_cost %></td>
            <td><%= u.b_outstanding %></td>
            <td><%= u.b_notes %></td>
            <td><%= u.r_no %></td>
            <td><%= u.checkin %></td>
            <td><%= u.checkout %></td>
        </tr>
        <% }  %>
    </tbody> 
</table>


<%- include('footer') %>